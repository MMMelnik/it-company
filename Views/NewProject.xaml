<Window x:Class="it_company.Views.NewProject"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:it_company.Views"
        mc:Ignorable="d"
         Title="NewProject" Height="450" Width="800" Style="{StaticResource MyWindowStyle}">
    <Grid>
        <Label Content="New project" HorizontalAlignment="Center" 
               Margin="0,26,0,0" VerticalAlignment="Top" FontSize="25"/>
        <Label Content="Project title: " HorizontalAlignment="Left" 
            Margin="52,102,0,0" VerticalAlignment="Top" FontSize="20"/>
        <Label Content="Manager: " HorizontalAlignment="Left" 
            Margin="52,164,0,0" VerticalAlignment="Top" FontSize="20"/>
        <TextBox HorizontalAlignment="Left" Margin="268,108,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="132" Height="24"/>
        <ComboBox HorizontalAlignment="Left" Margin="266,175,0,0" VerticalAlignment="Top" Width="132"/>
        <Button Content="Go to workspace" HorizontalAlignment="Left" Margin="27,386,0,0" VerticalAlignment="Top" Width="95" Height="25" Click="Button_Click"/>
        <Button Content="Save" HorizontalAlignment="Left" Margin="374,386,0,0" VerticalAlignment="Top" Width="95" Height="25"/>
    </Grid>
</Window>